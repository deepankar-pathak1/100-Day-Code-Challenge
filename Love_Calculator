# ðŸš¨ Don't change the code below ðŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ðŸš¨ Don't change the code above ðŸ‘†

#Write your code below this line ðŸ‘‡
x = "true"
y = "love"

name1 = name1.lower()
name2 = name2.lower()
a = 0
b = 0

for i in name1:
  for j in x:
    if i == j:
      a += 1
for j in name2:
  for p in x:
    if j == p:
      a += 1

for m in name1:
  for n in y :
    if m == n:
      b += 1
for l in name2:
  for h in y:
    if l == h:
      b += 1



print(f"Your Love Percentage is :{a}{b}% :) ")



