text = input("Please Enter a senetence : ").lower()

print(text)
lst = ["a","e","i","o","u"]

# lst1 = []

# for i in text:
#   lst1.append(i)
# # print(lst1)
# # print(lst)

# count = 0 
# for i in lst1:
#   for j in lst:
#     if i == j:
#       count = count + 1

# print(count)

count = 0
for i in text:
  for k in lst:
    if i == k:
      # print(i)
      count = count +1

print(f"There are {count} Number of Vowels Present in This Text {text}")
